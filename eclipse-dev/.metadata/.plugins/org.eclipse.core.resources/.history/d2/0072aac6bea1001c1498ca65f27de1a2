package pack2;
class Util
{
	static void sleep(long millis)
	{
		try
		{
			Thread.sleep(millis);
		}catch(InterruptedException ex)
		{
			ex.printStackTrace();
		}
	}
}
class A extends Thread
{
	public void run()
	{
		for(int i = 0; i< 1000; i++)
		{
			System.out.println(i);
		}
	}
}
public class M2 {

}
